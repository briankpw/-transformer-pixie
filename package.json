{
  "name": "pixie-transformer",
  "version": "1.0.3",
  "description": "With Pixie, user able to configure Dimension, Measurement(1 or Multiple), Filter, Sorting. Easy to generate to generate payload for chart and JSON payload. ",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node --inspect dist/",
    "build": "tsc",
    "test": "mocha -r ts-node/register test/*.ts",
    "delete": "@RD /S /Q dist",
    "retest": "npm run delete && npm run build && npm run test",
    "publish": "npm publish --access=public",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- --compilers ts:ts-node/register test/*.ts - - -R spec",
    "semantic-release": "semantic-release",
    "semantic-release-pre": "semantic-release pre",
    "semantic-release-post": "semantic-release post",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briankpw/pixie.git"
  },
  "bugs": {
    "url": "https://github.com/briankpw/pixie/issues"
  },
  "homepage": "https://github.com/briankpw/pixie#readme",
  "keywords": [
    "pixie",
    "json",
    "dimension",
    "measurement",
    "filter",
    "sort",
    "payload",
    "converting", 
    "transformer",
    "transform",
    "highcharts",
    "visualization",
    "tool"
  ],
  "author": "Brian Koh Ping Weng <briankpw@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "typings": "^2.1.1",
    "semantic-release": "^15.12.5",
    "travis-deploy-once": "^5.0.10"
  },
  "dependencies": {
    "@types/underscore": "^1.8.9",
    "underscore": "^1.9.1"
  }
}